package clasesUtiles;

public class Sumar {
	
	
	public int n1,n2,n3;
	
	public  Sumar(int n1,int n2,int n3)
	{
		this.n1=n1;
		this.n2=n2;
		this.n3=n3;
	}
	public  Sumar(int n1,int n2)
	{
		this.n1=n1;
		this.n2=n2;
		
	}
	public int sumas(int a,int b)
	{
		return a+b;
	}
	public int sumas(int a,int b,int c)
	{
		return a+b+c;
	}
	
	Sumar obj=new Sumar();
	public static void main(String[] args) {
		

	}

}

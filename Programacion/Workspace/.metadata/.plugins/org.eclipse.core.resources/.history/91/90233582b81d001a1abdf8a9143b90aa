package ArrayLists;

import java.util.ArrayList;

public class EjerciciosArrayList {

	public static void main(String[] args) {
		
		
		ArrayList<String> miAL=new ArrayList<>();
		
		miAL.add("Pepe");
		miAL.add("Juan");
		miAL.add("Luis");
		miAL.add("María");
		miAL.add("Fernando");
		
		System.out.println(miAL);
		
		
		miAL.removeIf(var -> (var.charAt(2) == 'a' || var.charAt(2)== 'r' ));
		
		
		System.out.println(miAL);
	}

}

class alumno
{
	String nombre;
	String apellido;
	int nia;
	int edad;
	
	public String getNombre()
	{
		return nombre;
	}
	public void setNombre(String _nombre)
	{
		nombre=_nombre;
	}
	public int getNIA()
	{
		return nia;
	}
	public void setNIA(int _nia)
	{
		nia= _nia;
	}
	
}
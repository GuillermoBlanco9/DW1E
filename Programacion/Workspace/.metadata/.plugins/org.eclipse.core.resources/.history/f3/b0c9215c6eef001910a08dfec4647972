import java.util.Random;
public class numeroAleatorio {
	
	
	public static void main(String[] args) 
	{
		
		
		Random numeroAlea=new Random(System.currentTimeMillis());
		
		int v;
		
		
		do
		{
			v=15+numeroAlea.nextInt(106);
			System.out.println(v);
			
			//v=numeroAlea.doubles(20.0'150.0);
		}while (v!=32);
		System.out.println("The last "+v);
		
		switch (v)
		{
		case 1:case 2:case 3:
			System.out.println(v+"hola");
			break;
		case 4:
			v++;
			break;
		
		default:
			v+=8;
		}
		
		
	}

}


public class BreakPoints {
    
	public static void main(String[] args)
	{
		
		
		
		
	}
}

class alumno
{
	
	String nombre;
	String apellido;
	int nia;
	int edad;

	
	
	public String getNombre()
	{
		return nombre;
	}
	public void setNombre(String _nombre)
	{
		nombre=_nombre;
	}
	public int getNIA()
	{
		return nia;
	}
	public void setNIA(int _nia)
	{
		nia= _nia;
	}
	
	public static void main(String[] args)
	{
		alumno a1=new alumno();
		
		a1.setNIA(1000);
		System.out.println(a1.getNIA());
		
		
	}
}